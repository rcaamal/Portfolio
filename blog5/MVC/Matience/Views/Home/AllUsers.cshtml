@model IEnumerable<Matience.Models.Tendent>

@{
    ViewBag.Title = "AllUsers";
}

<h2>AllUsers</h2>

<div>
    @Html.ActionLink("Add Another Form", "Create")
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.AptName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.UNumber)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Explanation)
        </th>

       
        
        

    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.AptName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.UNumber)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Explanation)
        </td>

        

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
            @Html.ActionLink("Details", "Details", new { id = item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ID })
        </td>

    </tr>
    }

</table>
